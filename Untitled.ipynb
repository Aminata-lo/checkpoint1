    "def my_age(a):\n",
    "    b = int(2021)\n",
    "    try:\n",
    "        while a in range(0, 2022):\n",
    "            age = b - a\n",
    "            return print(f\"vous avez actuellement {age} ans.\")\n",
    "\n",
    "    except a > 2021:\n",
    "        return print(\"impossible de calculer votre age.\")\n",
    "    \n",
    "    \n",
    "    \n",
    "my_age()    "
   ]
  }
